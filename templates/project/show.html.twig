{% extends 'base.html.twig' %}

{% block body %}
    <h1>{{ project.name }}</h1>
    <p>{{ project.description }}</p>
    <p>{{ project.id }}</p>
    <p>{{ project.author }}</p>
    <a href="{{ uploaded_asset(project.imagePath) }}" target="_blank">
        <img src="{{ project.imagePath|imagine_filter('squared_thumbnail_medium') }}">
    </a>
{% endblock %}

{% block title %}

{% endblock %}

